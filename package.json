    "test": "npm run migrate:test && DOTENV_CONFIG_PATH=./test.env jest",
  },
  "dependencies": {
    "@keystone-6/auth": "^8.0",
    "@keystone-6/core": "^6.0",
    "@keystone-6/fields-document": "^9.0",
    "nodemailer": "^6.9.14",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": "^14.15 || ^16.13 || ^18.7.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/nodemailer": "^6.4.15",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^8.0.0",
    "jest": "^29.7.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.9.4"
  }
